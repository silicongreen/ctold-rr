if @error.nil?
  page.replace_html 'form-errors', :text => ''
  page.replace_html 'save_batches_' + @main_batch_id, :partial => 'batches_data'
  page.replace_html 'flash-box', :text=>"<p class='flash-msg'>#{flash[:notice]}</p>" unless flash[:notice].nil?
  page << "Modalbox.hide();"
else
	page.replace_html 'form-errors', :partial => 'errors', :object => @batch_new
  page.visual_effect(:highlight, 'form-errors')
end