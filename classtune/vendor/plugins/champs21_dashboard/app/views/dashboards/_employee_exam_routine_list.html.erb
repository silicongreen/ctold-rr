<% unless routines.nil? %>
  <% routines.each do |h| %>

    <div class="portlet-subcontent">     
      <div class="timetable_left"><img width="30" src="/images/icons/subjects/<%= h['subject_icon'] %>" /></div>
      <div class="timetable_right"  style="margin-top:5px;">
        <div class="subcontent-date"><%= h['start_time'] %> - <%= h['end_time'] %></div>
        <div class="subcontent-header themed_text">
          <%= h['subject'] %> <%= h['exam_name'] %>
        </div>
        <div class="subcontent-date"><%= h['batch'] %></div>
      </div>       
    </div>
    <div style="border-bottom: 1px solid #ccc; float: left; margin: 15px; width: 90%;"></div> 

  <% end %>
<% else %>
  <div class="portlet-subcontent">    
    No Exam found
  </div>  
<% end  %>