                                  failed = false
                                  full_mark_mtt = full_mark_mtt + 1
                                  unless pass_mark[full_mark_cq.to_i].blank?
                                    if cq_obtain_mark.to_i <  pass_mark[full_mark_cq.to_i].to_i
                                      failed_grade = true
                                    end
                                  end
                                  unless pass_mark[full_mark_mcq.to_i].blank?
                                    if mcq_obtain_mark.to_i <  pass_mark[full_mark_mcq.to_i].to_i
                                      failed_grade = true
                                    end
                                  end
                                  unless pass_mark[full_mark_prac].blank?
                                    if prac_obtain_mark.to_i <  pass_mark[full_mark_prac].to_i
                                      failed_grade = true
                                    end
                                  end
                                  full_mark_mtt = full_mark_mtt - 1
                              else  
                                if full_mark_cq > 0
                                  if grade_mark_cq > 0
                                    point_mark = (grade_mark_cq.to_f/full_mark_cq.to_f)*100
                                    grade = GradingLevel.percentage_to_grade(point_mark, @batch.id)
                                    if !grade.blank? and !grade.name.blank? and !grade.credit_points.blank?
                                      if grade.credit_points.to_i == 0
                                        failed_grade = true
                                      end  
                                    end
                                  else
                                    failed_grade = true
                                  end  
                                end 
                              
                                if full_mark_mcq > 0
                                  if grade_mark_mcq > 0
                                    point_mark = (grade_mark_mcq.to_f/full_mark_mcq.to_f)*100
                                    grade = GradingLevel.percentage_to_grade(point_mark, @batch.id)
                                    if !grade.blank? and !grade.name.blank? and !grade.credit_points.blank?
                                      if grade.credit_points.to_i == 0
                                        failed_grade = true
                                      end  
                                    end
                                  else
                                    failed_grade = true
                                  end  
                                end 
                              
                                if full_mark_mtt > 0
                                  if grade_mark_mtt > 0
                                    point_mark = (grade_mark_mtt.to_f/full_mark_mtt.to_f)*100
                                    grade = GradingLevel.percentage_to_grade(point_mark, @batch.id)
                                    if !grade.blank? and !grade.name.blank? and !grade.credit_points.blank?
                                      if grade.credit_points.to_i == 0
                                        failed_grade = true
                                      end  
                                    end
                                  else
                                    failed_grade = true
                                  end  
                                end 
                              end
                              
                              
                              if @connect_exam_obj.result_type == 3
                                if cq_obtain_mark > 0
                                  cq_obtain_mark = cq_obtain_mark.to_f/2.00
                                  full_mark_cq = full_mark_cq.to_f/2.00
                                  cq_obtain_mark = cq_obtain_mark.round()
                                end  
                                if mcq_obtain_mark > 0
                                  mcq_obtain_mark = mcq_obtain_mark.to_f/2.00
                                  full_mark_mcq = full_mark_mcq.to_f/2.00
                                  mcq_obtain_mark = mcq_obtain_mark.round()
                                end
                                if mtt_obtain_mark > 0
                                  mtt_obtain_mark = mtt_obtain_mark.to_f/2.00
                                  full_mark_mtt = full_mark_mtt.to_f/2.00
                                  mtt_obtain_mark = mtt_obtain_mark.round()
                                end
                              end  
                              coverted_75 = 0
                              if @connect_exam_obj.result_type == 10
                                  full_mark_cq_mcq_prac = full_mark_cq+full_mark_mcq+full_mark_prac
                                  total_mark_cq_mcq_prac = cq_obtain_mark+mcq_obtain_mark+prac_obtain_mark
                                  if full_mark_cq_mcq_prac > 0
                                    coverted_75 = (total_mark_cq_mcq_prac.to_f/full_mark_cq_mcq_prac.to_f)*75
                                  end   
                                  subject_obtain_mark = coverted_75+mtt_obtain_mark
                                  subject_full_mark = 75+full_mark_mtt
                              else
                                subject_obtain_mark = cq_obtain_mark+mcq_obtain_mark+mtt_obtain_mark
                                subject_full_mark = full_mark_cq+full_mark_mcq+full_mark_mtt
                              end

                            %>
                              <% if subject_obtain_mark.to_i > 0 and subject_full_mark.to_i > 0 %>
                                <% main_mark = (subject_obtain_mark.to_f/subject_full_mark.to_f)*100 %>  
                              <% end %> 
                              <% if has_exam == true %>
                                <td>
                                    <% total_mark = total_mark+subject_obtain_mark.to_f %>
                                    <%= sprintf( "%0.02f", cq_obtain_mark) %>
                                    <hr/>
                                    <%= sprintf( "%0.02f", mcq_obtain_mark) %>
                                    <hr/>
                                    <% if @connect_exam_obj.result_type == 10 %>
                                    <%= sprintf( "%0.02f", prac_obtain_mark) %>
                                    <hr/>
                                    <%= sprintf( "%0.02f", coverted_75) %>
                                    <hr/>
                                    <% end %>
                                    <%= sprintf( "%0.02f", mtt_obtain_mark) %>
                                    <hr/>
                                    <%
                                    if subject_total_marks[sub['id'].to_i].blank?
                                      subject_total_marks[sub['id'].to_i] = 0
                                    end  
                                    subject_total_marks[sub['id'].to_i] = subject_total_marks[sub['id'].to_i]+main_mark
                                    %>
                                    <%= sprintf( "%0.02f", main_mark) %>
                                    <hr/>
                                    <%  grade = GradingLevel.percentage_to_grade(main_mark, @batch.id) %>
                                    <% if !grade.blank? and !grade.name.blank? %>
                                      <%
                                      if failed_grade == true
                                        grade.credit_points = 0.0
                                        grade.name = "F"
                                      end  
                                      if grade.credit_points.to_i == 0
                                        f_grade_count = f_grade_count+1
                                      end 
                                      total_grade = total_grade+grade.credit_points
                                      if subject_grades[sub['id'].to_i][grade.id.to_i].blank?
                                        subject_grades[sub['id'].to_i][grade.id.to_i] = 0
                                      end  
                                      subject_grades[sub['id'].to_i][grade.id.to_i] = subject_grades[sub['id'].to_i][grade.id.to_i]+1
                                      if grades.include?(grade.name.to_s)
                                        grade_count[grade.name.to_s] = grade_count[grade.name.to_s]+1
                                      else
                                        grades << grade.name.to_s
                                        grade_count[grade.name.to_s] = 1
                                      end  
                                    %>
                                      <%= grade.credit_points %>  
                                    <% else %>
                                      -  
                                    <% end %>
                                </td> 
                              <% end %>
                            <% end %>   
                          <% end %> 
                          <% @report_data['report']['no_exam_subject_resutl'].each do |sub| %>
                            <td>
                                <% if !sub['subject_comment'][std['id']].nil? && !sub['subject_comment'][std['id']].blank? %>
                                  <%
                                  if !sub['subject_comment'][std['id']].blank?
                                    if grades.include?(sub['subject_comment'][std['id']].to_s)
                                      grade_count[sub['subject_comment'][std['id']]] = grade_count[sub['subject_comment'][std['id']]]+1
                                      else
                                      grades << sub['subject_comment'][std['id']].to_s
                                      grade_count[sub['subject_comment'][std['id']].to_s] = 1
                                    end 
                                    end 
                                %>
                                  <%=  sub['subject_comment'][std['id']] %>
                                <% else %>
                                  &nbsp;
                                <% end %> 
                            </td>     

                          <% end %>       
                          <%
                          gpa_fraction = 0.0
                          if total_grade > 0 && subject_count > 0 && f_grade_count == 0
                            gpa_fraction = total_grade.to_f/subject_count.to_f
                            passed_student = passed_student+1
                          end  
                          
                          %>
                          <td>
                              <%= sprintf( "%0.02f", gpa_fraction) %>    
                          </td>
                          <td>
                              F[<%= f_grade_count %>]
                              <% if f_grade[f_grade_count].blank? %>
                              <% f_grade[f_grade_count] = 0 %>
                              <% end %>
                              <%  f_grade[f_grade_count] = f_grade[f_grade_count]+1 %>
                          </td> 



                      </tr>
                    <% end %>
                  <% end %>
                      <% if @connect_exam_obj.result_type.to_i == 10 %> 
                          <pagebreak></pagebreak>
                      <% else %>
                        <% if std_count>4 %>
                          <tr class="page-break-low">
                              <td colspan="100">&nbsp;</td>
                          </tr>
                        <% end %>
                      <% end %>
                      <tr class="table-header">
                          <td colspan="2" rowspan="8">Subjective Report</td>
                          <td>Average Marks(%)</td>
                          
                          <% @report_data['report']['subjects'].each do |sub| %>
                            <% has_exam = false %>
                            <% if @all_subject_id.include?(sub['id'].to_i) %>
                              <% has_exam = true %>
                            <% end %>
                            <% if has_exam %>
                              <% avg_mark = 0 %>
                              <% unless subject_total_marks[sub['id'].to_i].blank? %>
                                <% avg_mark = subject_total_marks[sub['id'].to_i].to_f/total_student_numbers.to_f %>
                              <% end %>
                              <td><%= sprintf( "%0.02f", avg_mark) %></td>
                                  
                            <% end %>
                          <% end %>
                              
                          <td colspan="2">Summary</td>
                      </tr>  
                      <% 
                      first = true
                      unless @grading_levels.empty?
                              @grading_levels.each_with_index do |grade,index|
                      %>
                      <tr class="table-header">
                       <td><%= grade.name %></td>  
                        <% @report_data['report']['subjects'].each do |sub| %>
                          <td><%= subject_grades[sub['id'].to_i][grade.id.to_i] %></td>
                        <% end %> 
                        <% if first == true %>
                          <td colspan="2" rowspan="7" >
                              <table class="no-border">
                                  <tr><td>F[0] = <%= f_grade[0] %></td><td>F[7] = <%= f_grade[7] %></td></tr>
                                  <tr><td>F[1] = <%= f_grade[1] %></td><td>F[8] = <%= f_grade[8] %></td></tr>
                                  <tr><td>F[2] = <%= f_grade[2] %></td><td>F[9] = <%= f_grade[9] %></td></tr>
                                  <tr><td>F[3] = <%= f_grade[3] %></td><td>F[10] = <%= f_grade[10] %></td></tr>
                                  <tr><td>F[4] = <%= f_grade[4] %></td><td>F[11] = <%= f_grade[11] %></td></tr>
                                  <tr><td>F[5] = <%= f_grade[5] %></td><td>F[12] = <%= f_grade[12] %></td></tr>
                                  <tr><td>F[6] = <%= f_grade[6] %></td><td>F[12+] = <%= f_grade[13] %></td></tr>
                              </table>
                          </td> 
                          <% first = false %>
                        <% end %>  
                      </tr>    
                      <%
                      end
                     end
                    %>
                    <% colspan = @report_data['report']['subjects'].count %>
                    <% colspan = colspan+3 %>
                    <tr class="table-header">
                       
                        <td colspan="<%= colspan %>" class="no-border-td" ></td> 
                        <td colspan="2" class="align-right">Total Student = <%= total_student_numbers %></td>
                    </tr>
                    <tr class="table-header">
                       
                        <td colspan="<%= colspan %>" class="no-border-td" ></td> 
                        <td colspan="2" class="align-right">Failed = <%= total_student_numbers-passed_student %></td>
                    </tr>
                    <tr class="table-header">
                       
                        <td colspan="<%= colspan %>" class="no-border-td" ></td> 
                        
                        <% pass = 100  %>
                        <% if total_student_numbers > 0 && passed_student < total_student_numbers  %>
                          <% pass = (passed_student.to_f/total_student_numbers.to_f)*100  %>
                        <% end %>
                        <td colspan="2" class="align-right">Pass = <%= sprintf( "%0.02f", pass) %>%</td>
                    </tr>
                      
              </table>
            <% end %>
        </center>
    </div>


</div>

