<div id="content-header">
<%= show_header_icon %>
  <h1><%= t('online_meetings') %></h1>
<div class='header-sep'>|</div>
<div class='sub-header'><%= "Recordings" %></div>
</div>
<div id="page-yield">
    <img src="images/classlive/Group 3467.png" width="100%">
  <% unless flash[:notice].nil? %>
    <p class="flash-msg"> <%= flash[:notice] %> </p>
  <% end %>
  <div class="box">
    <div id="event-table">
      <%= render :partial=>"recordings" %>
    </div>
    <div class="extender"></div>
  </div>

</div>
<style>
    .go_button{
      height: 34px !important;
      margin-left: 39px !important;
      margin-top: 5px !important;
    }
    .schedule {
      font-size: 13px;
      border: 2px solid #29363B;
      background-color: #29363B;
      padding: 6px 9px;
      color: #fff;
      border-radius: 6px;
      font-weight: 500;
      margin-bottom: 8px;
      cursor: pointer;
  }
  .table_join a {
      border: none;
      background: #1AAD9E;
      color: #fff;
      font-size: 12px;
      padding: 4px 17px;
      border-radius: 9px;
      text-transform: capitalize;
      margin-right: -6px;
      margin-left: -8px;
  }
  .links a{
      text-decoration: none;
  }
  .links a:hover{
      text-decoration: none;
  }
  .table_header_title h6 {
      font-size: 22px;
      font-weight: 500;
      color: #29363B;
      margin: 12px 1px;
  }
</style>