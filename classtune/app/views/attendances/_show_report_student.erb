<script type="text/javascript" src="/javascripts/swfobject.js"></script>
<div id="calendar" style="margin-top:0px; width: 100%;">
 <%= @graph %>
</div> 
<div class="legend-row">

    
    <div class="legend-wrapper">
      <div class="legend legend-holiday"></div>
      <div class="legend-text legend-holiday-text">Present <%=  @present %> Days</div>
    </div>

    <div class="legend-wrapper">
      <div class="legend legend-late"></div>
      <div class="legend-text legend-late-text">Late  <%=  @late %> Days</div>
    </div>

    <div class="legend-wrapper">
      <div class="legend legend-absent"></div>
      <div class="legend-text legend-absent-text">Absent <%=  @absent %> Days</div>
    </div>

    <div class="legend-wrapper">
      <div class="legend legend-leave"></div>
      <div class="legend-text legend-leave-text">Leave <%=  @leave %> Days</div>
    </div>

  </div>


