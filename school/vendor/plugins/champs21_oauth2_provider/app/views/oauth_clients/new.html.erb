<div id="content-header">
<%= show_header_icon %>
  <h1><%= t('client_apps') %></h1>
<div class='header-sep'>|</div>
<div class='sub-header'><%= t('new_text') %></div>

<div id="inner-tab-menu">
  <ul>
    <li class='themed_bg themed-dark-hover-background'><%= link_to t('client_apps'), oauth_clients_path %></li>
  </ul>
</div>

</div>
<div id="page-yield">
  <% unless flash[:notice].nil? %>
    <p class="flash-msg"> <%= flash[:notice] %> </p>
  <% end %>
  <div class="box">
    <%= render :partial => "form", :locals => { :oauth_client => @oauth_client } %>
    <div class="extender"></div>
  </div>
</div>

