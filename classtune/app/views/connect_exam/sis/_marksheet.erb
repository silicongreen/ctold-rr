<%= render  :partial=>"/connect_exam/sis/marksheet_"+@connect_exam_obj.result_type.to_s %>
<style>
    td.color-red
    {
        color:red !important;
    }


.even td{

    padding: 8px 3px 8px 3px;
    border: 1px solid #000000;
    border-right: 0px solid #fff;
    border-top: 0px solid #fff;
    border-bottom: 0;
    font-size: 18px;
}


.odd td{
    padding: 8px 8px 8px 8px;
    border: 1px solid #000000;
    border-right: 0px solid #fff;
    border-top: 0px solid #fff;
    border-bottom: 0;
    font-size: 18px;
}


.table-header td{
    padding: 8px 3px 8px 3px;
    border-top: 1px solid #000000;
    border-left: 1px solid #000000;
    border-right: 1px solid #000000;
    color: #000;
    background: #EEEEEE;
    font-size: 15px;
}
    
 </style>   