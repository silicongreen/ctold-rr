<% if MultiSchool.current_school.id == 340  %>
  <% if @connect_exam_obj.result_type == 1 or @connect_exam_obj.result_type == 2 or @connect_exam_obj.result_type == 3 %>
    <%= render  :partial=>"/connect_exam/sir_john_wilson_school/effot_gradesheet_1" %>
  <% elsif @connect_exam_obj.result_type == 4 or @connect_exam_obj.result_type == 7 %>
    <%= render  :partial=>"/connect_exam/sir_john_wilson_school/effot_gradesheet_2" %>
  <% elsif @connect_exam_obj.result_type == 6 or @connect_exam_obj.result_type == 11 %>
    <%= render  :partial=>"/connect_exam/sir_john_wilson_school/effot_gradesheet_3" %>
  <% end %>
<% else %>
  <%= render  :partial=>"/connect_exam/general_pdf" %>
<% end %>


