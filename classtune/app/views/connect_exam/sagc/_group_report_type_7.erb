
<% unless @report_data.blank?  or  @report_data['report'].blank? %>
            <%
              has_sb = false
              has_ob = false
              has_pr = false
              has_ct = false

            %>
<%
  if @student_list_first_term.blank?
    @subject_highest_1st_term = @subject_highest_2nd_term
    @student_position_first_term = @student_position_second_term
    @student_position_first_term_batch = @student_position_second_term_batch
  end


%>
              <table id="pdf-table" class="top-left-center" width="80%" cellspacing="0">
                  <% c = 'even'  %>
                  <% total_mark_of_allexam = 0 %>
                 
                  <tr class="even header-shade">
                      <td class="col-pdf" rowspan="2">
                          <b>Sl.</b>
                      </td>
                      <td class="col-pdf no-warp  align-left-baf" rowspan="2"><b><%= "Subjects" %></b></td>
                      <td class="col-pdf" rowspan="2">
                        <b>Full Marks</b>
                      </td>
                      <% colspan = 0 %>
                      <% @report_data['report']['exams'].each do |report| %>
                        <% if report['exam_category'] == '1' %>
                          <td rowspan="2"><b><%= report['exam_name'] %></b></td>
                          <% has_ct = true %>
                        <% else %>
                          <% if report['exam_category'] == '3' %>
                            <% has_sb = true %>
                          <% end %>
                          <% if report['exam_category'] == '4' %>
                            <% has_ob = true %>
                          <% end %>
                          <% if report['exam_category'] == '5' %>
                            <% has_pr = true %>
                          <% end %>
                          <% colspan = colspan+1 %>
                        <% end %>  
                      <% end %>
                       <%
                  class_nine_changes = false
  if has_ct
    if @batch.course.course_name.upcase == "NINE"
      class_nine_changes = true
    end
  end
                  %>
                      <% if class_nine_changes == false %>
                        <% colspan = colspan+2 %>
                      <% else %>
                        <% colspan = colspan+1 %>
                      <% end %>
                      <td class="col-pdf" colspan="<%= colspan %>" rowspan="1"><b>Examination</b></td>
                      <td class="col-pdf" rowspan="2"><b>Total Marks</b></td>
                      <td class="col-pdf" rowspan="2"><b>Highest Marks</b></td>
                      <td class="col-pdf" rowspan="2"><b>Letter Grade</b></td>
                      <td class="col-pdf" rowspan="2"><b>Grade Point</b></td>
                      
                  </tr>
                 
                  <tr class="even header-shade">
                      <% if has_ob == true %>
                            <td rowspan="1"><b>OB</b></td>
                      <% end %> 
                      <% if has_sb == true %>
                            <td rowspan="1"><b>SB</b></td>
                            <% if class_nine_changes %>
                              <td rowspan="1"><b>Avg</b></td>
                            <% end %>
                      <% end %> 
                      <% if has_pr == true %>
                            <td rowspan="1"><b>PR</b></td>
                      <% end %>
                      <% if class_nine_changes == false %>
                        <td rowspan="1"><b>Total</b></td>
                        <td rowspan="1"><b>Avg</b></td>
                      <% end %>
                  </tr>

                  <% total_grade = 0 %>
                  <% total_subject = 0 %>
                  <% total_subject_main = 0 %>
                  <% grand_total = 0 %>
                  <% grand_total_fraction = 0 %>
                  <% grade_point_avg = 0 %>
                  <% failed = false %>
                  <% already_done = false %>
                  <% unless @report_data['report']['subjects'].blank? %> 
                    <% @report_data['report']['subjects'].each do |subjects| %>
                      <% if subjects['grade_subject'].to_i == 1 %>
                        <% next %>
                      <% end %>
                      <% failed_subject = false %>
                      <%
                        total_sb = 0
                        total_ob = 0
                        total_pr = 0
                    
                        full_sb = 0
                        full_ob = 0
                        full_pr = 0
                      %> 
                      <% if already_done %>
                          <%
                            already_done = false
                            next
                          %>
                      <% end %>
                      <tr class="<%= cycle(c,(["odd","even"]-[c]).first) %>">
                          <% total_mark = 0 %>
                          <% full_mark = 0 %>
                          <%
                            third_subject = "";
                            fourth_subject = "";
                          %>
                          <% if !@std_subject_hash_type.blank? %>
                            <% if @std_subject_hash_type.include?(@student.id.to_s+"|||"+subjects['id'].to_s+"|||4") %>
                              <% fourth_subject = "(4th)" %>
                            <% end %>
                            <% if @std_subject_hash_type.include?(@student.id.to_s+"|||"+subjects['id'].to_s+"|||3") %>
                              <% third_subject = "(3rd)" %>
                            <% end %>
                          <% end %>
                          <% if fourth_subject.blank? %>
                            <% total_subject = total_subject+1 %>
                          <% end %>
                          <% total_subject_main = total_subject_main+1 %>
                          <td class="col-pdf"><%= total_subject_main %></td>
                          
                          
                          <td class="col-pdf no-warp align-left-baf"><%= subjects['name'] %><%= fourth_subject %><%= third_subject %></td>
                          <% unless @report_data['report']['exams'].blank? %>  
                            <% @report_data['report']['exams'].each do |report| %>   
                              <% if !report['result'].blank? and !report['result'][report['exam_id']].blank? and !report['result'][report['exam_id']][subjects['id']].blank?  and !report['result'][report['exam_id']][subjects['id']]['full_mark'].blank? %>
                                <% full_mark = full_mark+report['result'][report['exam_id']][subjects['id']]['full_mark'].to_i %>
                                <% if report['exam_category'] == '3' %>
                                  <% full_sb = full_sb+report['result'][report['exam_id']][subjects['id']]['full_mark'].to_i %>
                                <% end %>
                                <% if report['exam_category'] == '4' %>
                                  <% full_ob = full_ob+report['result'][report['exam_id']][subjects['id']]['full_mark'].to_i %>
                                <% end %>
                                <% if report['exam_category'] == '5' %>
                                  <% full_pr = full_pr+report['result'][report['exam_id']][subjects['id']]['full_mark'].to_i %>
                                <% end %>
                              <% end %> 
                            <% end %>
                          <% end %>


                          <% 
                            highest_mark = 0
                            if !@subject_highest_1st_term.blank? and !@subject_highest_1st_term[subjects['id'].to_i].blank?
                              highest_mark = @subject_highest_1st_term[subjects['id'].to_i].to_f
                            end
                          
                          %>
                          <% divider = 3 %>
                          <% if full_mark.to_f > 0 %>
                            <% full_mark_all = 50 %>
                            <% if full_mark.to_f >= 100 %>
                             <% full_mark_all = 100 %>
                             <td class="col-pdf">100</td>
                            <% else %> 
                              <% divider = 4 %>
                              <td class="col-pdf">50</td>
                            <% end %>
                            <% class_test_mark = 0 %>
                            <% class_test_full_mark = 0%>
                            <% unless @report_data['report']['exams'].blank? %>  
                            <% @report_data['report']['exams'].each do |report| %>   
                              <% if report['exam_category'] == '1' %>
                                <% if !report['result'].blank? and !report['result'][report['exam_id']].blank? and !report['result'][report['exam_id']][subjects['id']].blank?  and !report['result'][report['exam_id']][subjects['id']]['marks_obtained'].blank? %>                                   
                                  <%  class_test_mark = class_test_mark+report['result'][report['exam_id']][subjects['id']]['marks_obtained'].to_f %>
                                <% end %>
                                <% if !report['result'].blank? and !report['result'][report['exam_id']].blank? and !report['result'][report['exam_id']][subjects['id']].blank?  and !report['result'][report['exam_id']][subjects['id']]['full_mark'].blank? %>
                                    <% class_test_full_mark = class_test_full_mark+report['result'][report['exam_id']][subjects['id']]['full_mark'].to_f %>
                                  <% end %>
                              <% end %>
                            <% end %>
                            <% end %>
                            <% if class_test_full_mark.to_i == 30 && full_mark_all == 50 %>
                                <% divider = 6 %>
                            <% end %>
                            <% class_test_mark_before = class_test_mark %>
                            <% class_test_mark = (class_test_mark.to_f/divider.to_f) %>
                            <% class_test_mark = class_test_mark.round() %>
                            <% if has_ct %>
                              <td class="col-pdf no-warp"><%= class_test_mark %></td>
                            <% end %>
                                  
                            
                            <% main_exam_mark = 0 %>
                            <% unless @report_data['report']['exams'].blank? %>  
                            <% @report_data['report']['exams'].each do |report| %>   
                              <% if report['exam_category'] != '1' %>
                                <% if !report['result'].blank? and !report['result'][report['exam_id']].blank? and !report['result'][report['exam_id']][subjects['id']].blank?  and !report['result'][report['exam_id']][subjects['id']]['marks_obtained'].blank? %>                                   
                                  <%  main_exam_mark = main_exam_mark+report['result'][report['exam_id']][subjects['id']]['marks_obtained'].to_f %>
                                  <% if report['exam_category'] == '3' %>
                                    <% total_sb = total_sb+report['result'][report['exam_id']][subjects['id']]['marks_obtained'].to_f %>
                                  <% end %>
                                  <% if report['exam_category'] == '4' %>
                                    <% total_ob = total_ob+report['result'][report['exam_id']][subjects['id']]['marks_obtained'].to_f %>
                                  <% end %>
                                  <% if report['exam_category'] == '5' %>
                                    <% total_pr = total_pr+report['result'][report['exam_id']][subjects['id']]['marks_obtained'].to_f %>
                                  <% end %>
                                  <td class="col-pdf"><%= report['result'][report['exam_id']][subjects['id']]['marks_obtained'].to_f.round.to_i %></td>
                                  <% if class_nine_changes && report['exam_category'] == '3' %>
                                      <% sb_avg_marks = 0 %>
                                      <% class_test_mark_convert = class_test_full_mark.to_f/divider.to_f  %>
                                        <% sb_mark_convert = full_mark_all - full_ob - full_pr - class_test_mark_convert %>
                                        <%
                                        sb_avg_marks = 0
                                        if full_sb > 0 && total_sb > 0
                                          sb_avg_marks = (total_sb.to_f/full_sb.to_f)*sb_mark_convert
                                        end
                                        %>
                                        <td class="col-pdf no-warp">
                                            <%= sprintf( "%0.02f", sb_avg_marks)%>
                                          </td>
                                  <% end %>
                                  <% if  @part_by_pass_fail %>
                                    <% if report['result'][report['exam_id']][subjects['id']]['grade'] == "F" && fourth_subject.blank? && subjects['subject_group_id'].to_i == 0 %>
                                      <% if (report['result'][report['exam_id']][subjects['id']]['marks_obtained'].to_f.round.to_i != 11 || report['result'][report['exam_id']][subjects['id']]['full_mark'].to_i != 25) && (report['result'][report['exam_id']][subjects['id']]['marks_obtained'].to_f.round.to_i != 13 || report['result'][report['exam_id']][subjects['id']]['full_mark'].to_i != 30) %>
                                        <% failed = true %>
                                        <% failed_subject = true %>
                                      <% end %>
                                    <% end %>
                                    <% if report['result'][report['exam_id']][subjects['id']]['grade'] == "F" && !fourth_subject.blank? && subjects['subject_group_id'].to_i == 0 %>
                                      <% if (report['result'][report['exam_id']][subjects['id']]['marks_obtained'].to_f.round.to_i != 11 || report['result'][report['exam_id']][subjects['id']]['full_mark'].to_i != 25) && (report['result'][report['exam_id']][subjects['id']]['marks_obtained'].to_f.round.to_i != 13 || report['result'][report['exam_id']][subjects['id']]['full_mark'].to_i != 30) %>
                                        <% failed_subject = true %>
                                      <% end %>
                                    <% end %>
                                      
                                    <% if fourth_subject.blank? && subjects['subject_group_id'].to_i == 0 %>
                                      <% if report['result'][report['exam_id']][subjects['id']]['marks_obtained'].to_f.round.to_i == 31 && report['result'][report['exam_id']][subjects['id']]['full_mark'].to_i == 70 %>
                                        <% failed = true %>
                                        <% failed_subject = true %>
                                      <% end %>
                                    <% end %>
                                      
                                    <% if !fourth_subject.blank? && subjects['subject_group_id'].to_i == 0 %>
                                      <% if report['result'][report['exam_id']][subjects['id']]['marks_obtained'].to_f.round.to_i == 31 && report['result'][report['exam_id']][subjects['id']]['full_mark'].to_i == 70 %>
                  
                                        <% failed_subject = true %>
                                      <% end %>
                                    <% end %>
                                  <% end %>
                                <% else %>
                                  <td class="col-pdf">-</td>
                                <% end %>
                                  
                                  
                              <% end %>
                            <% end %>
                            <% end %>
                            <% if class_nine_changes == false %>
                            <td class="col-pdf no-warp">
                               <%= main_exam_mark.to_f.round.to_i %>
                            </td> 
                            <%  end %>
                             <% avg_mark = main_exam_mark.to_f*1 %> 
                            <% if has_ct %>
                              <% if @batch.course.course_name.upcase == "NINE" %>
                                  <% class_test_mark_convert = class_test_full_mark.to_f/divider.to_f  %>
                                  <% sb_mark_convert = full_mark_all - full_ob - full_pr - class_test_mark_convert %>
                                  <%
                                  sb_avg_marks = 0
                                  if full_sb > 0 && total_sb > 0
                                    sb_avg_marks = (total_sb.to_f/full_sb.to_f)*sb_mark_convert
                                  end 
                                  avg_mark = sb_avg_marks.to_f+total_ob+total_pr
                                  %>
                              <% else %>
                                <% avg_mark = main_exam_mark.to_f*0.9 %>
                              <% end %>
                            <% end %>
                           
                            <% if class_nine_changes == false %>     
                            <td class="col-pdf no-warp">
                               <%= sprintf( "%0.02f", avg_mark)%>
                            </td>
                            <% end %>
                            <% tota_mark_with_monthly = avg_mark+class_test_mark %>
                            
                              <td class="col-pdf no-warp">
                                  <b><%= tota_mark_with_monthly.to_f.round.to_i %></b>
                              </td>
                            <%
                              main_mark = (tota_mark_with_monthly.to_f.round/full_mark_all.to_f)*100
                            %>
                            <% grand_total_fraction = grand_total_fraction+tota_mark_with_monthly.to_f %>
                            <% grand_total = grand_total+tota_mark_with_monthly.to_f.round.to_i %>
                            <td class="col-pdf no-warp">
                               <%= highest_mark.to_f.round.to_i %>
                              <% if failed_subject == true && subjects['subject_group_id'].to_i > 0 %>
                                <%= "  -"  %>
                              <% end %>
                            </td>
                            <% if subjects['subject_group_id'].to_i > 0 %>
                              
                              <% @report_data['report']['subjects'].each do |subjects2| %>
                                <% if subjects2['subject_group_id'].to_i != subjects['subject_group_id'].to_i || subjects2['id'].to_i == subjects['id'].to_i  %>
                                  <% next %>
                                <% end %>
                                <% total_mark = 0 %>
                                <% full_mark = 0 %>
                                <%
                                  total_sb = 0
                                  total_ob = 0
                                  total_pr = 0
                              
                                  full_sb = 0
                                  full_ob = 0
                                  full_pr = 0
                                %> 
                                <% unless @report_data['report']['exams'].blank? %>  
                                  <% @report_data['report']['exams'].each do |report| %>   
                                    <% if !report['result'].blank? and !report['result'][report['exam_id']].blank? and !report['result'][report['exam_id']][subjects2['id']].blank?  and !report['result'][report['exam_id']][subjects2['id']]['full_mark'].blank? %>
                                      <% full_mark = full_mark+report['result'][report['exam_id']][subjects2['id']]['full_mark'].to_i %>
                                      
                                    <% end %> 
                                  <% end %>
                                <% end %>
                                <% unless @report_data['report']['exams'].blank? %>  
                                  <% @report_data['report']['exams'].each do |report| %>   
                                    <% if !report['result'].blank? and !report['result'][report['exam_id']].blank? and !report['result'][report['exam_id']][subjects['id']].blank?  and !report['result'][report['exam_id']][subjects['id']]['full_mark'].blank? %>
                                      <% full_mark = full_mark+report['result'][report['exam_id']][subjects['id']]['full_mark'].to_i %>
                                      <% if report['exam_category'] == '3' %>
                                        <% full_sb = full_sb+report['result'][report['exam_id']][subjects['id']]['full_mark'].to_i %>
                                      <% end %>
                                      <% if report['exam_category'] == '4' %>
                                        <% full_ob = full_ob+report['result'][report['exam_id']][subjects['id']]['full_mark'].to_i %>
                                      <% end %>
                                      <% if report['exam_category'] == '5' %>
                                        <% full_pr = full_pr+report['result'][report['exam_id']][subjects['id']]['full_mark'].to_i %>
                                      <% end %>
                                    <% end %> 
                                  <% end %>
                                <% end %>
                                <% divider = 4 %>
                                <% if full_mark.to_f > 0 %>
                                  <% full_mark_all = 50 %>
                                  <% if full_mark.to_f >= 100 %>
                                    <% divider = 3 %>
                                   <% full_mark_all = 100 %>    
                                  <% end %>
                                <% end %>
                                
                                <% class_test_mark = 0 %>
                                <% class_test_full_mark = 0 %>
                                <% unless @report_data['report']['exams'].blank? %>  
                                <% @report_data['report']['exams'].each do |report| %>   
                                  <% if report['exam_category'] == '1' %>
                                    <% if !report['result'].blank? and !report['result'][report['exam_id']].blank? and !report['result'][report['exam_id']][subjects2['id']].blank?  and !report['result'][report['exam_id']][subjects2['id']]['marks_obtained'].blank? %>                                   
                                      <%  class_test_mark = class_test_mark+report['result'][report['exam_id']][subjects2['id']]['marks_obtained'].to_f %>
                                    <% end %>
                                    <% if !report['result'].blank? and !report['result'][report['exam_id']].blank? and !report['result'][report['exam_id']][subjects['id']].blank?  and !report['result'][report['exam_id']][subjects['id']]['full_mark'].blank? %>
                                    <% class_test_full_mark = class_test_full_mark+report['result'][report['exam_id']][subjects['id']]['full_mark'].to_f %>
                                  <% end %>
                                  <% end %>
                                <% end %>
                                <% end %>
                                <% if class_test_full_mark.to_i == 30 && full_mark_all == 50 %>
                                    <% divider = 6 %>
                                <% end %>
                                <% class_test_mark = (class_test_mark.to_f/divider.to_f) %>
                                <% class_test_mark = class_test_mark.round() %>

                                <% main_exam_mark = 0 %>
                                <% unless @report_data['report']['exams'].blank? %>  
                                  <% @report_data['report']['exams'].each do |report| %>   
                                    <% if report['exam_category'] != '1' %>
                                      <% if !report['result'].blank? and !report['result'][report['exam_id']].blank? and !report['result'][report['exam_id']][subjects2['id']].blank?  and !report['result'][report['exam_id']][subjects2['id']]['marks_obtained'].blank? %>                                   
                                        <%  main_exam_mark = main_exam_mark+report['result'][report['exam_id']][subjects2['id']]['marks_obtained'].to_f %>
                                        <% if report['exam_category'] == '3' %>
                                          <% total_sb = total_sb+report['result'][report['exam_id']][subjects2['id']]['marks_obtained'].to_f %>
                                        <% end %>
                                        <% if report['exam_category'] == '4' %>
                                          <% total_ob = total_ob+report['result'][report['exam_id']][subjects2['id']]['marks_obtained'].to_f %>
                                        <% end %>
                                        <% if report['exam_category'] == '5' %>
                                          <% total_pr = total_pr+report['result'][report['exam_id']][subjects2['id']]['marks_obtained'].to_f %>
                                        <% end %>
                                      <% end %>
                                    <% end %>
                                  <% end %>
                                  
                                <% end %>
                                <% if  @part_by_pass_fail %>
                                <% if full_sb > 0 %>
                                  <%
                                    
                                    if total_sb > 0
                                      sb_main = (total_sb.to_f/full_sb.to_f)*100
                                      grade = GradingLevel.percentage_to_grade(sb_main, @batch.id)
                                      if !grade.blank? and !grade.name.blank?
                                        if (grade.credit_points.to_i == 0 && fourth_subject.blank?) or (total_sb.to_i < 64 && full_sb.to_i == 140 && fourth_subject.blank?) or (total_sb.to_i < 90 && full_sb.to_i == 200 && fourth_subject.blank?)
                                          failed_subject = true
                                        end
                                      end   
                                    else
                                      failed_subject = true
                                    end    
                                
                                  %>
                                <% end %>
                                <% if full_ob > 0 %>
                                  <%
                                    
                                    if total_ob > 0
                                      ob_main = (total_ob.to_f/full_ob.to_f)*100
                                      grade = GradingLevel.percentage_to_grade(ob_main, @batch.id)
                                      if !grade.blank? and !grade.name.blank? && (total_ob != 22 || full_ob != 25)
                                        if (grade.credit_points.to_i == 0 && fourth_subject.blank?) && (total_ob != 26 || full_ob != 60)
                                          failed_subject = true
                                        end
                                      end   
                                    else
                                      failed_subject = true
                                    end    
                                
                                  %>
                                <% end %>
                                <% if full_pr > 0 %>
                                  <%
                                    
                                    if total_pr > 0
                                      pr_main = (total_pr.to_f/full_pr.to_f)*100
                                      grade = GradingLevel.percentage_to_grade(pr_main, @batch.id)
                                      if !grade.blank? and !grade.name.blank?
                                        if grade.credit_points.to_i == 0 && fourth_subject.blank? && (total_pr != 22 || full_pr != 25)
                                          failed_subject = true
                                        end
                                      end   
                                    else
                                      failed_subject = true
                                    end    
                                
                                  %>
                                <% end %> 
                                <% end %>
                                <% avg_mark = main_exam_mark.to_f %> 
                                <% if has_ct %>
                                  <% if @batch.course.course_name.upcase == "NINE" %>
                                      <% class_test_mark_convert = class_test_full_mark.to_f/divider.to_f  %>
                                      <% sb_mark_convert = full_mark_all - full_ob - full_pr - class_test_mark_convert %>
                                      <%
                                      sb_avg_marks = 0
                                      if full_sb > 0 && total_sb > 0
                                        sb_avg_marks = (total_sb.to_f/full_sb.to_f)*sb_mark_convert
                                      end 
                                      avg_mark = sb_avg_marks.to_f+total_ob+total_pr
                                      %>
                                  <% else %>
                                    <% avg_mark = main_exam_mark.to_f*0.9 %>
                                  <% end %>
                                <% end %>
                                <% tota_mark_with_monthly = avg_mark+class_test_mark  %>
                                <%
                                  main_mark2 = (tota_mark_with_monthly.to_f.round/full_mark_all.to_f)*100
                                  main_mark = (main_mark.to_f+main_mark2.to_f)/2.00
                                %>
                                <%  grade = GradingLevel.percentage_to_grade(main_mark, @batch.id) %>
                                <% if !grade.blank? and !grade.name.blank? %>
                            
                                  <% if grade.credit_points.to_i == 0 && fourth_subject.blank? %>
                                    <% failed = true %>
                                    <% failed_subject = true %>
                                  <% end %>
                                  <% if  failed_subject == true %>
                                    <% grade.credit_points = 0.0 %>
                                    <% grade.name = "F" %>
                                  <% end %>
                            
                                  <% if fourth_subject.blank? %>
                                    <% total_grade = total_grade.to_f+grade.credit_points.to_f %>
                                  <% else %>
                                    <% new_grade_point = grade.credit_points.to_f-2 %>
                                    <% if new_grade_point > 0 %>
                                     <% total_grade = total_grade.to_f+new_grade_point.to_f %>
                                    <% end %>  
                                  <% end %>
                                  <td class="col-pdf" rowspan="2"><%= grade.name %></td>
                                  <td class="col-pdf" rowspan="2"><%= grade.credit_points %></td>
                                <% else %>
                                  <td class="col-pdf" rowspan="2">-</td>
                                  <td class="col-pdf" rowspan="2">-</td>
