<div class="label-field-pair">
  <label for="student_grade"><%= t('associated_data') %></label>
</div>
<div class="fee_category_scroll">
  <% (@associated_models + @join_models).each do |model| %>
    <div class="each_batch">
      <%= check_box_tag("associated_models[]",model.second, false,
        :onchange => remote_function(
          :before => "get_model_names()",
          :url => { :controller => "exports",:action => "show_associated_columns",:model => @model},
          :with => "'status='+this.checked+'&model_names='+get_model_names()"),:class => "right")%>
  <%#= check_box_tag "associated_models[]", model.second, { :onchange => remote_function( :url => { :action => 'show_associated_columns', :columns => Export.prepare_associated_columns(@model,model.second) })},{:class=>'message_check' } %>
      <label for="model_<%= model.first %>"><%= model.first.to_s %></label>
    </div>
  <% end %>
</div>


