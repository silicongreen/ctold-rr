<% if MultiSchool.current_school.id == 2 or MultiSchool.current_school.id == 312 %>
  <%= render  :partial=>"/connect_exam/south_point/tabulation" %> 
<% elsif MultiSchool.current_school.id == 323  %>
    <%= render  :partial=>"/connect_exam/baf_shaheen/tabulation" %>
<% elsif MultiSchool.current_school.id == 246  %>
    <%= render  :partial=>"/connect_exam/cherry_blossoms/tabulation" %>
<% elsif MultiSchool.current_school.id == 325  %>
    <%= render  :partial=>"/connect_exam/mirpur_english/tabulation" %>
<% elsif MultiSchool.current_school.id == 280  %>
    <%= render  :partial=>"/connect_exam/st_joseph/tabulation" %>
<% elsif MultiSchool.current_school.id == 342  %>
    <%= render  :partial=>"/connect_exam/sjis/tabulation" %>
<% elsif File.exists?(Rails.root.join("app", "views", "connect_exam",MultiSchool.current_school.code, "_tabulation.erb")) %>
    <%= render  :partial=>"/connect_exam/#{MultiSchool.current_school.code}/tabulation" %>
<% else %>
  <%= render  :partial=>"/connect_exam/tabulation" %>
<% end %>
