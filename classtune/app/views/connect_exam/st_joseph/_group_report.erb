
<%  
    @dt_year = @connect_exam_obj.created_at.strftime("%Y") 
    @dt_start = @dt_year.to_s+"-01-01 00:00:00"
    @dt_end   = @dt_year.to_s+"-12-31 00:00:00"
    @student_detention_count = Detention.count(:all,:conditions=>{:student_id=>@student.id,:created_at => @dt_start..@dt_end})

%>
<%= render  :partial=>"/connect_exam/st_joseph/group_report_type_"+@connect_exam_obj.result_type.to_s %>

