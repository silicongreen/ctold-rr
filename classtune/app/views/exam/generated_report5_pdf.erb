<%
  if !@report_data.blank? && !@report_data['report'].blank? && !@report_data['total_new_std'].blank?
    @report_data['total'] = @report_data['total_new_std']
  end
%>
<% if MultiSchool.current_school.id != 357 %>
  <%= render  :partial=>"connect_result_reformate" %>
<% end %>
<% if MultiSchool.current_school.id == 283 %>
  <%= render  :partial=>"/connect_exam/academia/group_report" %>
<% elsif MultiSchool.current_school.id == 246 %>
  <%= render  :partial=>"/connect_exam/cherry_blossoms/group_report" %>
<% elsif MultiSchool.current_school.id == 280 %>
  <%= render  :partial=>"/connect_exam/st_joseph/group_report" %>
<% elsif MultiSchool.current_school.id == 275 %>
  <%= render  :partial=>"/connect_exam/gateway/group_report" %>
<% elsif MultiSchool.current_school.id == 12  %>
  <%= render  :partial=>"/connect_exam/mpsc/group_report" %>
<% elsif MultiSchool.current_school.id == 186 %>
  <%= render  :partial=>"/connect_exam/akmis/group_report" %>
<% elsif MultiSchool.current_school.id == 319 %>
  <%= render  :partial=>"/connect_exam/sfx/group_report" %>
<% elsif MultiSchool.current_school.id == 323 %>
  <%= render  :partial=>"/connect_exam/baf_shaheen/group_report" %>
<% elsif MultiSchool.current_school.id == 312  %>
  <%= render  :partial=>"/connect_exam/south_point/group_report" %>
<% elsif MultiSchool.current_school.id == 325  %>
  <%= render  :partial=>"/connect_exam/mirpur_english/group_report" %>
<% elsif MultiSchool.current_school.id == 324  %>
  <%= render  :partial=>"/connect_exam/sagc/group_report" %>
<% elsif MultiSchool.current_school.id == 340  %>

  <% if @connect_exam_obj.result_type == 11 or @connect_exam_obj.result_type == 6 %>
    <%= render  :partial=>"/connect_exam/sir_john_wilson_school/first_term_report_for_10_to_12" %> 
  <% elsif @connect_exam_obj.result_type == 15 %>
    <%= render  :partial=>"/connect_exam/sir_john_wilson_school/report_for_mock" %>
  <% elsif @connect_exam_obj.result_type == 9 or @connect_exam_obj.result_type == 4 %>
    
    <%= render  :partial=>"/connect_exam/sir_john_wilson_school/term_report_for_5_to_9" %> 
  <% elsif @connect_exam_obj.result_type == 10 %>
    
    <%= render  :partial=>"/connect_exam/sir_john_wilson_school/final_term_report_for_5_to_9" %>
  <% elsif @connect_exam_obj.result_type == 13 %>
    <%= render  :partial=>"/connect_exam/sir_john_wilson_school/term_report_for_KG1_to_KG2" %>
  <% elsif @connect_exam_obj.result_type == 14 %>
    <%= render  :partial=>"/connect_exam/sir_john_wilson_school/term_report_for_C1_to_C4" %>
  <% else %>
    <%= render  :partial=>"/connect_exam/sir_john_wilson_school/single_report" %> 
  <% end %>
<% elsif MultiSchool.current_school.id == 324 or MultiSchool.current_school.id == 2  %>
  <%= render  :partial=>"/connect_exam/sjis/group_report" %>
<% elsif MultiSchool.current_school.id == 357  %>
  <%= render  :partial=>"/connect_exam/bncd/group_report" %>
<% elsif File.exists?(Rails.root.join("app", "views", "connect_exam",MultiSchool.current_school.code, "_group_report.erb")) %>
    <%= render  :partial=>"/connect_exam/#{MultiSchool.current_school.code}/group_report" %>
<% else %>
  <%= render  :partial=>"/connect_exam/general_pdf" %>
<% end %>