<% if @connect_exam_obj.result_type.to_i == 9 && @connect_exam_obj.quarter_number == 6 && @connect_exam_obj.result_type.to_i != 9 %>
<%
  if @tabulation_data.blank?
    student_response = get_tabulation_connect_exam(@connect_exam_obj.id,@batch.id,true)
    @tabulation_data = []
    if student_response['status']['code'].to_i == 200
      @tabulation_data = student_response['data']
    end
  end
%>
<%= render  :partial=>"/connect_exam/sis/finding_data" %>
<% end %>
<% @section_in_charge = "rezina-begum"  %>
  <% if @batch.full_name.index("Dhanmondi") %>
    <% @section_in_charge = "rowshan-islam"  %>
  <% end %>
 <% if (@batch.full_name.index("Mohammadpur") || @batch.full_name.index("Morning (January-December)")) && ( @batch.course_name == "VI" or @batch.course_name == "VII" or @batch.course_name == "VIII" or @batch.course_name == "IX"  or @batch.course_name == "X"  or @batch.course_name == "A Level" or @batch.course_name == "AS January 2019" or @batch.course_name == "AS July 2019") %>
    <% @section_in_charge = "mercy-sanabam"  %>
 <% end %>
<% @covid_main = 0 %>
<% year_month = I18n.l(@connect_exam_obj.published_date,:format=>"%Y-%m") %>
<% if year_month.to_s >= "2020-07" and year_month.to_s <= "2021-12"  %>
  <% @covid_main = 1 %>
<% end %>

<% unless @transcript.blank? %>
  <%= render  :partial=>"/connect_exam/sis/group_report_pdf_type_transcript_"+@connect_exam_obj.result_type.to_s %>
<% else %>
  <%= render  :partial=>"/connect_exam/sis/group_report_pdf_type_"+@connect_exam_obj.result_type.to_s %>
<% end %>
<style>
    #page-yield .height-50
    {
        height: 110px !important;
    }
    .sis table tr.odd td, .sis table tr.even td
    {
        font-weight:bold;
    }
    #page-yield .height-55
    {
        height: 60px !important;
    }
    
</style> 

