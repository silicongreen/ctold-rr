<div class="text-input-bg" style="float:left; margin-top: 7px; margin-bottom: 10px;">
     <%= calendar_date_select_tag "end_date","", :readonly=> true,:onchange => "#{remote_function(
          :url => { :action => 'report',"start_date"=>@start_date },
