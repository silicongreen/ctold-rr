                                      <% if total_sb > 0 %>
                                        <% converted_sb = (total_sb.to_f/full_sb.to_f)*converted_sb_full %>
                                        
                                      <% end %>
                                      <% if report['result'][report['exam_id']][subjects2['id']]['marks_obtained'].to_s == "AB" %>
                                        <td class="col-pdf">AB</td>
                                        <td class="col-pdf">-</td>
                                      <% else %>
                                        <td class="col-pdf"><%= report['result'][report['exam_id']][subjects2['id']]['marks_obtained'].to_f.round.to_i %></td>
                                        <td class="col-pdf"><%= converted_sb.round.to_i %></td>
                                      <% end %>
                                    <% end %>
                                    <% if report['exam_category'] == '4' %>
                                      <% total_ob = total_ob+report['result'][report['exam_id']][subjects2['id']]['marks_obtained'].to_f %>
                                      
                                      <% converted_ob_full = 100-converted_sb_full-full_pr %>
                                    
                                      <% if total_ob > 0 %>
                                        <% converted_ob = (total_ob.to_f/full_ob.to_f)*converted_ob_full %>
                                      <% end %>
                                      <% if report['result'][report['exam_id']][subjects2['id']]['marks_obtained'].to_s == "AB" %>
                                        <td class="col-pdf">AB</td>
                                        <td class="col-pdf">-</td>
                                      <% else %>
                                        <td class="col-pdf"><%= report['result'][report['exam_id']][subjects2['id']]['marks_obtained'].to_f.round.to_i %></td>
                                        <td class="col-pdf"><%= converted_ob.round.to_i %></td>
                                      <% end %>
                                    <% end %>
                                    <% if report['exam_category'] == '5' %>
                                      <% total_pr = total_pr+report['result'][report['exam_id']][subjects2['id']]['marks_obtained'].to_f %>
                                      <td class="col-pdf"><%= report['result'][report['exam_id']][subjects2['id']]['marks_obtained'].to_f.round.to_i %></td>
                                    <% end %>
                                    
                                      
                                  <% else %>
                                    <% if report['exam_category'] == '5' %>
                                    <td class="col-pdf">-</td>
                                  <% else %>
                                    <td class="col-pdf">-</td>
                                    <td class="col-pdf">-</td>
                                  <% end %>
                                  <% end %>
                                <% end %>
                              <% end %>
                              <% end %>  
                              <% avg_mark = total_pr+converted_ob+converted_sb %>

                              <% tota_mark_with_monthly = avg_mark %>
