                            <% end %> 
                            <%
                              if double_subject_start == true
                                two_sub_mark = two_sub_mark+subject_obtain_mark
                              end
                              subject_obtain_mark = (subject_obtain_mark.to_f/subject_full_mark.to_f)*100
                              subject_obtain_mark = subject_obtain_mark.round()
                              failed_student = false
                              if mcq_total == 25 && mcq.round.to_i < 10 && fourth_subject.blank?
                                failed_student = true
                              elsif mcq_total == 30 && mcq.round.to_i < 12 && fourth_subject.blank?
                                failed_student = true
                              end
            
                              if cq_total == 50 && cq.round.to_i < 20 && fourth_subject.blank?
                                failed_student = true
                              elsif cq_total == 70 && cq.round.to_i < 28 && fourth_subject.blank?
                                failed_student = true
                              end
            
                              if pr_total == 25 && pr.round.to_i < 10 && fourth_subject.blank?
                                failed_student = true
                              end
                              
                              
                            %>
                            <td>
                                <%= subject_obtain_mark.to_i %>   
                            </td>
                            
                            <% if sub['grade_subject'].to_i != 1 %>
                              <% if fourth_subject.blank? %>
                              <% total_mark = total_mark+subject_obtain_mark.to_f %>
                              <% else %>
                                <% 
                                  new_mark = subject_obtain_mark.to_f.round.to_i-40
                                  if new_mark > 0
                                    total_mark = total_mark+new_mark
                                  end
                                  
                                  %>
                               <% end %>   
                            <% end %>
                            <% main_mark = subject_obtain_mark.to_f %>
                            <% if count_grade == true %>
                              <%
                                 main_mark = 0
                                 if two_sub_mark > 0
                                    main_mark = (two_sub_mark.to_f/200)*100
                                 end   
                              %>
                            <% end %>
                            <%  if failed_student %>
                            <%  grade = GradingLevel.percentage_to_grade(0, @batch.id) %>
                            <%  else %>
                            <%  grade = GradingLevel.percentage_to_grade(main_mark, @batch.id) %>
                            <%  end %>
                            
                            <% if !grade.blank? and !grade.name.blank? %>
                              <% if double_subject_start == false || count_grade == true %>
                                <% if sub['grade_subject'].to_i != 1 %>
                                  <% if fourth_subject.blank? %>
                                    <% total_grade = total_grade+grade.credit_points.to_f %>
                                  <% else %>
                                    <% new_grade_point = grade.credit_points.to_f-2 %>
                                    <% if new_grade_point > 0 %>
                                      <% total_grade = total_grade.to_f+new_grade_point.to_f %>
                                    <% end %>
                                  <% end %>  
                                <% end %>
                              <% end %>

                              <td><%= sprintf( "%0.01f", grade.credit_points.to_f) %> 
                                <% if double_subject_start == false || count_grade == true %>
                                  
                                  <%
                                    double_subject_start = false
                                    count_grade = false 
                                    two_sub_mark = 0
                                  %>
                                <% end %>     
                              </td>
                            <% end %>
                            <% if sub['grade_subject'].to_i != 1 %>
                            <% full_mark_all = full_mark_all+subject_full_mark.to_f  %>
                            <% end %>
                        
                          <% end %>   
                        <% end %> 
                       <% percentage = 0 %>      
                        <% if full_mark_all > 0 &&  total_mark > 0 %>
                          <% percentage = (total_mark.to_f/full_mark_all.to_f)*100 %> 
                        <% end %>    

                          <td>
                              <%#= sprintf( "%0.02f", total_mark) %>    
                              <%= total_mark.round.to_i %>    
                          </td>
                          

                          <td>
                              <%= sprintf( "%0.02f", total_grade) %>
                          </td>
                          <% grade_point_avg = 0.00 %>
                          <% if total_grade > 0 && subject_count > 0 %>
                            <% grade_point_avg = total_grade.to_f/subject_count.to_f %>
                            <% if grade_point_avg > 5 %>
                              <% grade_point_avg = 5.00 %>
                            <% end %>  
                          <% end %> 
                          <% grade_point_avg = (grade_point_avg.to_f*100).to_f.round() %>
                          <% grade_point_avg = grade_point_avg.to_f/100 %>
                          <td>
                              <%= sprintf( "%0.02f", grade_point_avg) %>
                          </td> 
                          <td style="display: none;">
                            <% if !@student_position_batch[std['id'].to_i].blank? %> 
                              Pass
                            <% else %>
                              Fail
                            <% end %>
                          </td> 
                          <td>
                              <% if !@student_position_batch[std['id'].to_i].blank? %> 
                                <%= @student_position_batch[std['id'].to_i] %>
                              <% else %>
                                -
                              <% end %>
                          </td>
                          <td>
                              <% if !@student_position[std['id'].to_i].blank? %> 
                                <%= @student_position[std['id'].to_i] %>
                              <% else %>
                                -
                              <% end %>
                          </td>


                    </tr>
                    <% end %>
                  <% end %>
              </table>
            <% end %>
        </center>
    </div>


</div>

