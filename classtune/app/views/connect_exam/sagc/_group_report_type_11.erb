                                      <% if report['exam_category'] == '3' %>
                                        <% full_sb = full_sb+report['result'][report['exam_id']][subjects2['id']]['full_mark'].to_i %>
                                      <% end %>
                                      <% if report['exam_category'] == '4' %>
                                        <% full_ob = full_ob+report['result'][report['exam_id']][subjects2['id']]['full_mark'].to_i %>
                                      <% end %>
                                      <% if report['exam_category'] == '5' %>
                                        <% full_pr = full_pr+report['result'][report['exam_id']][subjects2['id']]['full_mark'].to_i %>
                                      <% end %>
                                    <% end %> 
                                  <% end %>
                                <% end %>
                                <% if full_mark.to_f > 0 %>
                                  <% full_mark_all = 50 %>
                                  <% if full_mark.to_f >= 100 %>
                                   <% full_mark_all = 100 %>    
                                  <% end %>
                                  <% if full_mark.to_f == 75 %>
                                   <% full_mark_all = 75 %>    
                                  <% end %>
                                <% end %>
                                

                                <% main_exam_mark = 0 %>
                                <% unless @report_data['report']['exams'].blank? %>  
                                  <% @report_data['report']['exams'].each do |report| %>   
                                    <% if report['exam_category'] != '1' %>
                                      <% if !report['result'].blank? and !report['result'][report['exam_id']].blank? and !report['result'][report['exam_id']][subjects2['id']].blank?  and !report['result'][report['exam_id']][subjects2['id']]['marks_obtained'].blank? %>                                   
                                        <%  main_exam_mark = main_exam_mark+report['result'][report['exam_id']][subjects2['id']]['marks_obtained'].to_f %>
                                        <% if report['exam_category'] == '3' %>
                                          <% total_sb = total_sb+report['result'][report['exam_id']][subjects2['id']]['marks_obtained'].to_f %>
                                        <% end %>
                                        <% if report['exam_category'] == '4' %>
                                          <% total_ob = total_ob+report['result'][report['exam_id']][subjects2['id']]['marks_obtained'].to_f %>
                                        <% end %>
                                        <% if report['exam_category'] == '5' %>
                                          <% total_pr = total_pr+report['result'][report['exam_id']][subjects2['id']]['marks_obtained'].to_f %>
                                        <% end %>
                                      <% end %>
                                    <% end %>
                                  <% end %>
                                <% end %>
                            
                                <% if full_sb > 0 %>
                                  <%
                                    
                                    if total_sb > 0
                                      sb_main = (total_sb.to_f/full_sb.to_f)*100
                                      grade = GradingLevel.percentage_to_grade(sb_main, @batch.id)
                                      if !grade.blank? and !grade.name.blank?
                                        if (grade.credit_points.to_i == 0 && fourth_subject.blank?) || (total_sb < 34 && full_sb == 100 && fourth_subject.blank? )
                                          failed_subject = true
                                        end
                                      end   
                                    else
                                      failed_subject = true
                                    end    
                                
                                  %>
                                <% end %>
                                <% if full_ob > 0 %>
                                  <%
                                    
                                    if total_ob > 0
                                      ob_main = (total_ob.to_f/full_ob.to_f)*100
                                      grade = GradingLevel.percentage_to_grade(ob_main, @batch.id)
                                      if !grade.blank? and !grade.name.blank? && (total_ob != 16 || full_ob != 50)
                                        if (grade.credit_points.to_i == 0 && fourth_subject.blank?)
                                          failed_subject = true
                                        end
                                      end   
                                    else
                                      failed_subject = true
                                    end    
                                
                                  %>
                                <% end %>
                                <% if full_pr > 0 %>
                                  <%
                                    
                                    if total_pr > 0
                                      pr_main = (total_pr.to_f/full_pr.to_f)*100
                                      grade = GradingLevel.percentage_to_grade(pr_main, @batch.id)
                                      if !grade.blank? and !grade.name.blank? && (total_ob != 16 || full_ob != 50)
                                        if grade.credit_points.to_i == 0 && fourth_subject.blank?
                                          failed_subject = true
                                        end
                                      end   
                                    else
                                      failed_subject = true
                                    end    
                                
                                  %>
                                <% end %>
                                <% avg_mark = main_exam_mark.to_f %>
                                <% tota_mark_with_monthly = avg_mark %>
                                <%
                                  main_mark2 = (tota_mark_with_monthly.to_f.round/full_mark_all.to_f)*100
                                  main_mark = (main_mark.to_f+main_mark2.to_f)/2.00
                                %>
                                <%  grade = GradingLevel.percentage_to_grade(main_mark, @batch.id) %>
                                <% if !grade.blank? and !grade.name.blank? %>
                                  <% if grade.credit_points.to_i == 0 && fourth_subject.blank? %>
                                    <% failed = true %>
                                    <% failed_subject = true %>
                                  <% end %>
                                  <% if  failed_subject == true %>
                                    <% grade.credit_points = 0.0 %>
                                    <% grade.name = "F" %>
                                  <% end %>
                            
                                  
                                  <% if fourth_subject.blank? %>
                                    <% total_grade = total_grade.to_f+grade.credit_points.to_f %>
                                  <% else %>
                                    <% new_grade_point = grade.credit_points.to_f-2 %>
                                    <% if new_grade_point > 0 %>
                                     <% total_grade = total_grade.to_f+new_grade_point.to_f %>
                                    <% end %>  
                                  <% end %>
                                  <td class="col-pdf" rowspan="2"><%= grade.name %></td>
                                  <td class="col-pdf" rowspan="2"><%= grade.credit_points %></td>
                                <% else %>
                                  <td class="col-pdf" rowspan="2">-</td>
                                  <td class="col-pdf" rowspan="2">-</td>
                                <% end %>
                               <% end %>
                            <% else %>
                            
                              <%  grade = GradingLevel.percentage_to_grade(main_mark, @batch.id) %>
                              <% if !grade.blank? and !grade.name.blank? %>
                                <% if grade.credit_points.to_i == 0 && fourth_subject.blank? %>
                                  <% failed = true %>
                                  <% failed_subject = true %>
                              <% end %>
                              <% if  failed_subject == true %>
                                <% grade.credit_points = 0.0 %>
                                <% grade.name = "F" %>
                              <% end %>
                              
                                <% if fourth_subject.blank? %>
                                    <% total_grade = total_grade.to_f+grade.credit_points.to_f %>
                                <% else %>
                                  <% new_grade_point = grade.credit_points.to_f-2 %>
                                  <% if new_grade_point > 0 %>
                                   <% total_grade = total_grade.to_f+new_grade_point.to_f %>
                                  <% end %>  
                                <% end %>
                                <td class="col-pdf"><%= grade.name %></td>
                                <td class="col-pdf"><%= grade.credit_points %></td>
                              <% else %>
                                <td class="col-pdf">-</td>
                                <td class="col-pdf">-</td>
                              <% end %>
                            <% end %>
                          <% else  %>
                            <td class="col-pdf" colspan="100">-</td>
                          <% end %>      

                      </tr> 
                      <% if subjects['subject_group_id'].to_i > 0 %>
                          <% @report_data['report']['subjects'].each do |subjects2| %>
                      
                          
                          <% if subjects_done.include?(subjects2['id'].to_i) %>
                              <%
                                next
                              %>
                          <% end %>
                          <% if subjects2['subject_group_id'].to_i != subjects['subject_group_id'].to_i || subjects2['id'].to_i == subjects['id'].to_i  %>
                            <% next %>
                          <% end %>
                          <% subjects_done << subjects2['id'].to_i %>
                          <tr class="<%= cycle(c,(["odd","even"]-[c]).first) %>">
                            <% total_mark = 0 %>
                            <% full_mark = 0 %>
                              <%
                                third_subject = "";
                                fourth_subject = "";
                              %>
                              <% if !@std_subject_hash_type.blank? %>
                                <% if @std_subject_hash_type.include?(@student.id.to_s+"|||"+subjects2['id'].to_s+"|||4") %>
                                  <% fourth_subject = "(4th)" %>
                                <% end %>
                                <% if @std_subject_hash_type.include?(@student.id.to_s+"|||"+subjects2['id'].to_s+"|||3") %>
                                  <% third_subject = "(3rd)" %>
                                <% end %>
                              <% end %>
                            <% total_subject_main = total_subject_main+1 %>
                            <td class="col-pdf"><%= total_subject_main %></td>
                            <td class="col-pdf no-warp align-left-baf"><%= subjects2['name'] %><%= fourth_subject %><%= third_subject %></td>
                            <% unless @report_data['report']['exams'].blank? %>  
                              <% @report_data['report']['exams'].each do |report| %>   

                                <% if !report['result'].blank? and !report['result'][report['exam_id']].blank? and !report['result'][report['exam_id']][subjects2['id']].blank?  and !report['result'][report['exam_id']][subjects2['id']]['full_mark'].blank? %>
                                  <% full_mark = full_mark+report['result'][report['exam_id']][subjects2['id']]['full_mark'].to_i %>
                                <% end %> 
                              <% end %>
                            <% end %>


                            <% 
                              highest_mark = 0
                              if !@subject_highest_1st_term.blank? and !@subject_highest_1st_term[subjects2['id'].to_i].blank?
                                highest_mark = @subject_highest_1st_term[subjects2['id'].to_i].to_f
                              end

                            %>

                            <% if full_mark.to_f > 0 %>
                              <% full_mark_all = 50 %>
                              <% if full_mark.to_f >= 100 %>
                               <% full_mark_all = 100 %>
                               <td class="col-pdf">100</td>
                              <% elsif full_mark.to_f == 75 %>  
                               <% full_mark_all = 75 %>
                               <td class="col-pdf">75</td>
                              <% else %>  
                                <td class="col-pdf">50</td>
                              <% end %>
                            

                              <% main_exam_mark = 0 %>
                              <% unless @report_data['report']['exams'].blank? %>  
                              <% @report_data['report']['exams'].each do |report| %>   
                                <% if report['exam_category'] != '1' %>
                                  <% if !report['result'].blank? and !report['result'][report['exam_id']].blank? and !report['result'][report['exam_id']][subjects2['id']].blank?  and !report['result'][report['exam_id']][subjects2['id']]['marks_obtained'].blank? %>                                   
                                    <%  main_exam_mark = main_exam_mark+report['result'][report['exam_id']][subjects2['id']]['marks_obtained'].to_f %>
                                    <% if report['result'][report['exam_id']][subjects2['id']]['marks_obtained'].to_s == "AB" %>
                                      <td class="col-pdf">AB</td>
                                    <% else %>
                                      <td class="col-pdf"><%= report['result'][report['exam_id']][subjects2['id']]['marks_obtained'].to_f.round.to_i %></td>
                                    <% end %>
                                  <% else %>
                                    <td class="col-pdf">-</td>
                                  <% end %>
                                <% end %>
                              <% end %>
                              <% end %>
                             
                              <% avg_mark = main_exam_mark.to_f %>

                              <% tota_mark_with_monthly = avg_mark %>
                              <td class="col-pdf no-warp">
                                  <b><%= tota_mark_with_monthly.to_f.round.to_i %></b>
                              </td>
                              <%
                                main_mark = (tota_mark_with_monthly.to_f.round/full_mark_all.to_f)*100
                              %>
                              <% grand_total_fraction = grand_total_fraction+tota_mark_with_monthly.to_f %>
                              <% grand_total = grand_total+tota_mark_with_monthly.to_f.round.to_i %>
                              <td class="col-pdf no-warp">
                                 <%= highest_mark.to_f.round.to_i %>
                              </td>
                              <%  grade = GradingLevel.percentage_to_grade(main_mark, @batch.id) %>
                              <% if !grade.blank? and !grade.name.blank? %>
                                <% if grade.credit_points.to_i == 0 && fourth_subject.blank? %>
                                 
                                <% end %>
                              <% else %>
                              <% end %>
                            <% else  %>
                              <td class="col-pdf" colspan="100">-</td>
                            <% end %>      

                        </tr> 
                      <% end %>
                      
                      <% end %>
                    <% end %>  
                  <% end %>
                      
                  <% if total_grade > 0 && total_subject > 0 %>
                      <% grade_point_avg = total_grade.to_f/total_subject.to_f %>
                      <% if grade_point_avg > 5 %>
                        
                        <% grade_point_avg = 5.00 %>
                      <% end %>  
