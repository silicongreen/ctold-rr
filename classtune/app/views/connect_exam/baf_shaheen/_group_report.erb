<% if @connect_exam_obj.result_type.to_i == 4 or @connect_exam_obj.result_type.to_i == 2 or @connect_exam_obj.result_type.to_i == 6 or @connect_exam_obj.result_type.to_i == 9 or @connect_exam_obj.result_type.to_i == 10 or @connect_exam_obj.result_type.to_i == 13  or @connect_exam_obj.result_type.to_i == 14 %> 
<%
      if @tabulation_data.nil?
        student_response = get_tabulation_connect_exam(@connect_exam_obj.id,@batch.id,true,false,params[:unsolved_exam])
        @tabulation_data = []
        if student_response['status']['code'].to_i == 200
          @tabulation_data = student_response['data']
        end
      end
%>
  <% if @connect_exam_obj.result_type.to_i == 9 %>
    <%= render  :partial=>"/connect_exam/baf_shaheen/finding_data2" %>
  <% elsif @connect_exam_obj.result_type.to_i == 10 %>
    <%= render  :partial=>"/connect_exam/baf_shaheen/finding_data3" %>
  <% elsif @connect_exam_obj.result_type.to_i == 13 %>
    <%= render  :partial=>"/connect_exam/baf_shaheen/finding_data4" %>
  <% elsif @connect_exam_obj.result_type.to_i == 14 %>
    <%= render  :partial=>"/connect_exam/baf_shaheen/finding_data5" %>
  <% else %>
    <%= render  :partial=>"/connect_exam/baf_shaheen/finding_data" %>
  <% end %>
<% end %>
<% if @current_user.student? or if @current_user.parent? %>
<%= render  :partial=>"/connect_exam/baf_shaheen/group_report_type_"+@connect_exam_obj.result_type.to_s %>
<% else %>
<%= render  :partial=>"/connect_exam/baf_shaheen/group_report_pdf_type_"+@connect_exam_obj.result_type.to_s %>
<% end %>


<style>
.baf_shaheen tr.odd td
{
    padding: 2px 2px 2px 9px !important;  
}
.baf_shaheen tr.even td
{
    padding: 2px 2px 2px 9px !important; 
}
</style>    