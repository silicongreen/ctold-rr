                                  <% next %>
                                <% end %>
                                <% total_mark = 0 %>
                                <% full_mark = 0 %>
                                <%
                                full_ob_previous = full_ob
                                total_ob_previous = total_ob
                                total_sb_previous = total_sb
                                converted_sb_full2 = 50
                                converted_ob_full2 = 25
                                full_sb = 0
                                full_ob = 0
                                full_pr2 = 0
                                total_sb = 0
                                total_ob = 0
                                total_pr2 = 0;

                                converted_sb2 = 0
                                converted_ob2 = 0
                                
                                %>

                                
                                <% unless @report_data['report']['exams'].blank? %>  
                                  <% @report_data['report']['exams'].each do |report| %>   
                                    <% if !report['result'].blank? and !report['result'][report['exam_id']].blank? and !report['result'][report['exam_id']][subjects2['id']].blank?  and !report['result'][report['exam_id']][subjects2['id']]['full_mark'].blank? %>
                                      <% full_mark = full_mark+report['result'][report['exam_id']][subjects2['id']]['full_mark'].to_i %>
                                      <% if report['exam_category'] == '3' %>
                                        <% full_sb = full_sb+report['result'][report['exam_id']][subjects2['id']]['full_mark'].to_i %>
                                      <% end %>
                                      <% if report['exam_category'] == '4' %>
                                        <% full_ob = full_ob+report['result'][report['exam_id']][subjects2['id']]['full_mark'].to_i %>
                                      <% end %>
                                      <% if report['exam_category'] == '5' %>
                                        <% full_pr = full_pr+report['result'][report['exam_id']][subjects2['id']]['full_mark'].to_i %>
                                        <% full_pr2 = full_pr2+report['result'][report['exam_id']][subjects2['id']]['full_mark'].to_i %>
                                      <% end %>
                                    <% end %> 
                                  <% end %>
                                <% end %>

                                <% full_mark_all = 100 %>

                                <% main_exam_mark = 0 %>
                                <% unless @report_data['report']['exams'].blank? %>  
                                  <% @report_data['report']['exams'].each do |report| %>   
                                    <% if report['exam_category'] != '1' %>
                                      <% if !report['result'].blank? and !report['result'][report['exam_id']].blank? and !report['result'][report['exam_id']][subjects2['id']].blank?  and !report['result'][report['exam_id']][subjects2['id']]['marks_obtained'].blank? %>                                   
                                        <%  main_exam_mark = main_exam_mark+report['result'][report['exam_id']][subjects2['id']]['marks_obtained'].to_f %>
                                        <% if report['exam_category'] == '3' %>
                                          <% total_sb = total_sb+report['result'][report['exam_id']][subjects2['id']]['marks_obtained'].to_f %>
                                          <% if full_sb == 40 %>
                                              <% converted_sb_full2 = 70 %>
                                          <% end %>
                                          <% if full_sb == 50 %>
                                              <% converted_sb_full2 = 100 %>
                                          <% end %>
                                          <% if total_sb > 0 %>
                                            <% converted_sb2 = (total_sb.to_f/full_sb.to_f)*converted_sb_full2 %>
                                            
                                          <% end %>
                                        <% end %>
                                        <% if report['exam_category'] == '4' %>
                                          <% total_ob = total_ob+report['result'][report['exam_id']][subjects2['id']]['marks_obtained'].to_f %>
                                    
                                          <% converted_ob_full2 = 100-converted_sb_full2-full_pr2 %>
                                        
                                          <% if total_ob > 0 %>
                                            <% converted_ob2 = (total_ob.to_f/full_ob.to_f)*converted_ob_full2 %>
                                          <% end %>
                                        <% end %>
                                        <% if report['exam_category'] == '5' %>
                                          <% 
                                          total_pr = total_pr+report['result'][report['exam_id']][subjects2['id']]['marks_obtained'].to_f
                                          total_pr2 =  report['result'][report['exam_id']][subjects2['id']]['marks_obtained'].to_f
                                          %>
                                          
                                        <% end %>
                                      <% end %>
                                    <% end %>
                                  <% end %>
                                <% end %>
                                <%
                                converted_sb_main = 0
                                converted_ob_main = 0
                                marks_second = total_pr2+converted_ob2+converted_sb2
                                prev_double_subject_marks = double_subject_marks
                                double_subject_marks = double_subject_marks+marks_second.round
                                %>
                                <% if full_sb > 0 %>
                                  <%
                                    
                                    if total_sb > 0 || total_sb_previous > 0
                                      converted_sb_full_main = converted_sb_full2+converted_sb_full
                                      converted_sb_main = converted_sb2.round+converted_sb.round
                                      sb_main = (converted_sb_main.round.to_f/converted_sb_full_main.to_f)*100
                                      grade = GradingLevel.percentage_to_grade(sb_main, @batch.id)
                                      if !grade.blank? and !grade.name.blank?
                                        if grade.credit_points.to_i == 0 || (converted_sb_main.round < 34 && converted_sb_full_main == 100)
                                            failed_subject = true
                                        end
                                      end   
                                    else
                                      failed_subject = true
                                    end    
                                
                                  %>
                                <% end %>
                                <% if full_ob_previous > 0 %>
                                  <%

                                    if total_ob_previous > 0 || total_ob > 0
                                      converted_ob_full_main = converted_ob_full2+converted_ob_full
                                      converted_ob_main = converted_ob2+converted_ob
                                      if (converted_ob_main.round < 16 && converted_ob_full_main == 50) || (converted_ob_main.round < 10 && converted_ob_full_main == 30) || (converted_ob_main.round < 20 && converted_ob_full_main == 60)
                                         failed_subject = true
                                      end   
                                    else
                                      failed_subject = true
                                    end
                                
                                  %>
                                <% end %>
                                <% if full_pr > 0 %>
                                  <%
                                    if total_pr > 0
                                      if total_pr < 16
                                        failed_subject = true
                                      end  
                                    else
                                      failed_subject = true
                                    end    
                                
                                  %>
                                <% end %>
                                <% avg_mark = converted_sb_main+converted_ob_main+total_pr %>
                                
                                <% tota_mark_with_monthly = avg_mark %>
                                <%
                                  main_mark = (double_subject_marks.round.to_f/200)*100
                                %>
                                <%  grade = GradingLevel.percentage_to_grade(main_mark, @batch.id) %>
                                <% if !grade.blank? and !grade.name.blank? %>
                                  <% if grade.credit_points.to_i == 0 && fourth_subject.blank? %>
                                    <% failed = true %>
                                    <% failed_subject = true %>
                                  <% end %>
                                  <% if  failed_subject == true %>
                                    <% grade.credit_points = 0.0 %>
                                    <% grade.name = "F" %>
                                  <% end %>
                            
                                  
                                  <% if fourth_subject.blank? %>
                                    <% total_grade = total_grade.to_f+grade.credit_points.to_f %>
                                  <% else %>
                                    <% new_grade_point = grade.credit_points.to_f-2 %>
                                    <% if new_grade_point > 0 %>
                                     <% total_grade = total_grade.to_f+new_grade_point.to_f %>
                                    <% end %>  
                                  <% end %>
                                  <td class="col-pdf" rowspan="2"><%= grade.name %></td>
                                  <td class="col-pdf" rowspan="2"><%= grade.credit_points %></td>
                                <% else %>
                                  <td class="col-pdf" rowspan="2">-</td>
                                  <td class="col-pdf" rowspan="2">-</td>
                                <% end %>
                               <% end %>
                            <% else %>
                            
                              <%  grade = GradingLevel.percentage_to_grade(main_mark, @batch.id) %>
                              <% if !grade.blank? and !grade.name.blank? %>
                                <% if grade.credit_points.to_i == 0 && fourth_subject.blank? %>
                                  <% failed = true %>
                                  <% failed_subject = true %>
                              <% end %>
                              <% if  failed_subject == true %>
                                <% grade.credit_points = 0.0 %>
                                <% grade.name = "F" %>
                              <% end %>
                              
                                <% if fourth_subject.blank? %>
                                    <% total_grade = total_grade.to_f+grade.credit_points.to_f %>
                                <% else %>
                                  <% new_grade_point = grade.credit_points.to_f-2 %>
                                  <% if new_grade_point > 0 %>
                                   <% total_grade = total_grade.to_f+new_grade_point.to_f %>
                                  <% end %>  
                                <% end %>
                                <td class="col-pdf"><%= grade.name %></td>
                                <td class="col-pdf"><%= grade.credit_points %></td>
                              <% else %>
                                <td class="col-pdf">-</td>
                                <td class="col-pdf">-</td>
                              <% end %>
                            <% end %>
                          <% else  %>
                            <td class="col-pdf" colspan="100">-</td>
                          <% end %>      

                      </tr> 
