  if student_response['status']['code'].to_i == 200
    @tabulation_data = student_response['data']
  end
  %>
  <%= render  :partial=>"/connect_exam/nascd/finding_data" %>
  <%
end
%>
<% if !@batch.course.course_name.upcase.index("PLAY").nil? or !@batch.course.course_name.upcase.index("NURSERY").nil? or !@batch.course.course_name.upcase.index("KG").nil?  %>
