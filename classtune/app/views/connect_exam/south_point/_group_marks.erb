<% if @connect_exam_obj.result_type.to_i == 1 or  @connect_exam_obj.result_type.to_i == 2 or  @connect_exam_obj.result_type.to_i == 4  %>
  <%
      student_response = get_tabulation_connect_exam(@connect_exam_obj.id,@batch.id,false)
      @tabulation_data = []
      if student_response['status']['code'].to_i == 200
        @tabulation_data = student_response['data']
      end
  %>
  <%= render  :partial=>"/connect_exam/south_point/highest_marks" %>
<% end %>
<% if @connect_exam_obj.result_type.to_i == 8 or @connect_exam_obj.result_type.to_i == 9  %>
  <%
      if @tabulation_data.blank?
        student_response = get_tabulation_connect_exam(@connect_exam_obj.id,@batch.id,false)
        @tabulation_data = []
        if student_response['status']['code'].to_i == 200
          @tabulation_data = student_response['data']
        end
      end
  %>
  <%= render  :partial=>"/connect_exam/south_point/highest_marks2" %>
<% end %>
<%= render  :partial=>"/connect_exam/south_point/group_report_type_"+@connect_exam_obj.result_type.to_s %>