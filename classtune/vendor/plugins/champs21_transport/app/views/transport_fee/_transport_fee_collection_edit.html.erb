  </div>
  <div class="label-field-pair">
    <label for="reason"><%= t('due_date') %></label>
    <div class="text-input-bg"><%= calendar_date_select_tag 'fee_collection[due_date]', I18n.l(@transport_fee_collection.due_date,:format=>"%d %b %Y"),:readonly=>true,:popup=>"force"%></div>
  </div>

  <div class="extender"></div>
  <div id="submit-button">
