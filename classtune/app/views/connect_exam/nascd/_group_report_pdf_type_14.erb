                                        <% total1 = total1+cq1 %>
                                        <% t1 = t1+cq1 %>
                                      <% end %>
                                      <% if report['exam_category'] == '4' %>
                                        <% mcq1 = report['result'][report['exam_id']][subjects['id']]['marks_obtained'].to_f%>
                                        <% mcq1_total = mcq1_total+mcq1 %>
                                        <% total1 = total1+mcq1 %>
                                        <% t1 = t1+mcq1 %>
                                        
                                      <% end %>
                                  <% end %>
                                  <% if report['quarter'] == '2' %>
