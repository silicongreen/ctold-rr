<% if @connect_exam_obj.result_type.to_i == 2 or @connect_exam_obj.result_type.to_i == 4 or @connect_exam_obj.result_type.to_i == 6 or @connect_exam_obj.result_type.to_i == 8 or @connect_exam_obj.result_type.to_i == 10  %> 
  <%
  if @tabulation_data.nil?
    student_response = get_tabulation_connect_exam(@connect_exam_obj.id,@batch.id,true)
    @tabulation_data = []
    if student_response['status']['code'].to_i == 200
      @tabulation_data = student_response['data']
    end
  end
%>
  
 <%= render  :partial=>"/connect_exam/sagc/finding_data_5" %> <%= stylesheet_link_tag 'list_table' %>
  <style>
      td.extra_height
      {
          height: 54px;
          vertical-align: top !important;
      }
      div.baf_shaheen
      {
          border:none;
      }
      .baf_shaheen td.align-right
      {
          text-align: right !important;
      }
      .baf_shaheen td.align-left
      {
          text-align: left !important;
      }
      .baf_shaheen table.tbl-lign-left tr.even td,.baf_shaheen table.tbl-lign-left tr.odd td
      {
          font-weight: normal !important;
          padding: 4px !important;
          font-size: 12px !important;
      }

      .baf_shaheen table.tbl-lign-left-3 tr.even td,.baf_shaheen table.tbl-lign-left-3 tr.odd td
      {
          font-weight: normal !important;
          padding: 2px 2px !important;
      }
      .baf_shaheen table.tbl-lign-left-3
      {
          float:left !important;
          margin-left: 2% !important;
      }

      .baf_shaheen table.tbl-lign-left-2 tr.even td, .baf_shaheen table.tbl-lign-left-2 tr.odd td
      {
          font-weight: normal !important;
          padding: 2px 2px !important;
          font-size: 10px !important;
      }

      div.baf_shaheen table.extra-padding tr.even td
      {
          padding: 7px 3px !important;
          font-size: 10px !important;
      }
      div.baf_shaheen table.extra-padding tr.even:last-child td
      {
          padding: 7px 3px !important;
      }


      .baf_shaheen table.tbl-lign-left-2
      {
          float:left !important;
          margin-left: 0.4% !important;
      }


      .baf_shaheen table.tbl-lign-left
      {
          float:left !important;
          margin-left: 2% !important;
      }
      .baf_shaheen table.first_table
      {
          margin-left: 0% !important;
      }
      .baf_shaheen div.grade_information
      {
          margin-left:2%;
          float:left;
          width: 30%;
          font-size: 10px;

      }
      .baf_shaheen div.grade_information table#pdf-table tr.even td
      {
          padding: 5px 4px !important;
      }
      div.baf_shaheen h2
      {
          font-weight: normal !important;
          font-size: 17px !important;
      }
      div.baf_shaheen table.top-left-center
      {
          float:left !important;
          margin-top:10px !important;
          margin-left: 0% !important;
          width: 99% !important;
      }
      div.baf_shaheen table.extra_bottom
      {
          margin-bottom: 10px;
      }
      div.baf_shaheen tr.odd td
      {
          padding: 1px 1px !important;  
          border: 1px solid #000000;
          font-size:10px;
      }
      div.baf_shaheen tr.even td
      {
          padding: 1px 1px !important;
          border: 1px solid #000000;
          font-size:10px;
      }
      div.baf_shaheen tr.extra-padding-total td
      {
          padding: 7px !important;
      }
      .top-margin-20
      {
          margin-top:10px !important;
      }
      .letter-grade tr td
      {
          height: 115px;
          font-size: 13px !important;
      }
      div.baf_shaheen table#pdf-table tr.header-shade
      {
          background: #e8e9ed !important;
      }
      .line
      {
          float:left;
          clear:both;
          margin-top:10px;
          margin-bottom:10px;
          border-bottom: 2px solid red;
          margin-left: 4% !important;
          width: 90% !important
      }
      .letter-grade tr td.font-size-45
      {
          font-size:20px !important;
      }
      .page-break-grade td{
          height: 5px;
          padding: 0px !important;
          border: 1px solid #fff;
          border-top: 1px solid Black;
          border-bottom: 1px solid Black;
      }
      .baf_shaheen tr.odd td
      {
          padding: 2px 1px 2px 2px !important;  
      }
      .baf_shaheen tr.even td
      {
          padding: 2px 1px 2px 2px !important;
      }

  </style> 
<% else %>
  <%
  if @tabulation_data.nil?
    student_response = get_tabulation_connect_exam(@connect_exam_obj.id,@batch.id,true)
    @tabulation_data = []
    if student_response['status']['code'].to_i == 200
      @tabulation_data = student_response['data']
    end
    end 
%>
  <% if @connect_exam_obj.result_type.to_i == 13 or @connect_exam_obj.result_type.to_i == 14 or @connect_exam_obj.result_type.to_i == 15 or @connect_exam_obj.result_type.to_i == 16 %>
    <%= render  :partial=>"/connect_exam/sagc/finding_data_6" %>
  <% elsif @connect_exam_obj.result_type.to_i == 17 %> 
    <%= render  :partial=>"/connect_exam/sagc/finding_data_25" %>
  <% elsif @connect_exam_obj.result_type.to_i == 18 %> 
    <%= render  :partial=>"/connect_exam/sagc/finding_data_18" %>
  <% elsif @connect_exam_obj.result_type.to_i == 19 %> 
    <%= render  :partial=>"/connect_exam/sagc/finding_data_19" %>
  <% elsif @connect_exam_obj.result_type.to_i == 11 %> 
    <%= render  :partial=>"/connect_exam/sagc/finding_data_6" %>
  <% else %>
    <%= render  :partial=>"/connect_exam/sagc/finding_data_5" %>
  <% end %>
  <%= stylesheet_link_tag 'list_table' %>
  <style>
      div.baf_shaheen
      {
          border:none;
      }
      .baf_shaheen td.align-right
      {
          text-align: right !important;
      }
      .baf_shaheen td.align-left
      {
          text-align: left !important;
      }
      .baf_shaheen table.tbl-lign-left tr.even td,.baf_shaheen table.tbl-lign-left tr.odd td
      {
          font-weight: normal !important;
          padding: 8px !important;
      }

      .baf_shaheen table.tbl-lign-left
      {
          float:left !important;
          width: 55% !important;
          margin-left: 4% !important;
      }
      .baf_shaheen div.grade_information
      {
          margin-left:3%;
          float:left;
          width: 31.7%;
          font-size: 14px;

      }
      .baf_shaheen div.grade_information table#pdf-table tr.even td
      {
          padding: 8px !important;
      }
      div.baf_shaheen h2
      {
          font-weight: normal !important;
          font-size: 32px !important;
      }
      div.baf_shaheen table.top-left-center
      {
          float:left !important;
          margin-top:30px !important;
          margin-left: 4% !important;
          width: 90.8% !important;
      }
      div.baf_shaheen tr.odd td
      {
          padding: 5px 10px !important;  
          border: 1px solid #000000;
          font-size:21px;
      }
      div.baf_shaheen tr.even td
      {
          padding: 5px 10px !important;
          border: 1px solid #000000;
          font-size:21px;
      }
      div.baf_shaheen table#pdf-table tr.header-shade td
      {
          padding: 5px 3px !important;
      }
      .top-margin-20
      {
          margin-top:30px !important;
      }
      .letter-grade tr td
      {
          font-size: 21px !important;
      }
      div.baf_shaheen table#pdf-table tr.header-shade
      {
          background: #e8e9ed !important;
      }
      .line
      {
          float:left;
          clear:both;
          margin-top:40px;
          margin-bottom:20px;
          border-bottom: 2px solid red;
          margin-left: 4% !important;
          width: 90% !important
      }
      .letter-grade tr td.font-size-45
      {
          font-size:45px !important;
      }
      .page-break-grade td{
          height: 10px;
          border: 1px solid #fff;
          border-top: 1px solid Black;
          border-bottom: 1px solid Black;
      }
      .baf_shaheen tr.odd td
      {
          padding: 2px 2px 2px 9px !important;  
      }
      .baf_shaheen tr.even td
      {
          padding: 2px 2px 2px 9px !important; 
      }

  </style> 
<% end %>
<%= render  :partial=>"/connect_exam/sagc/group_report_pdf_type_"+@connect_exam_obj.result_type.to_s %> 